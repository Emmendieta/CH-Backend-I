<head>
    <link rel="stylesheet" href="/css/Home.css">
</head>

<div>
    <h1 id="homeh2Title">Cart</h1>
</div>
<div>
    <h2 id="homeh2Title">Products in Cart:</h2>
</div>
<div>
    <ul id="homeUlProducts">
        {{#each products}}
        <li class="homeUlLi">
            <div class="homeUlLiDiv">
                <h3 class="homeUlLih3">Title: </h3>
                <h3 class="homeUlLih3">{{title}}</h3>
            </div>
            <div class="homeUlLiDiv">
                <h4 class="homeUlLih4">Quantity: </h4>
                <h4 class="homeUlLih4">{{quantity}}</h4>
            </div>
        </li>
        {{/each}}
    </ul>
</div>